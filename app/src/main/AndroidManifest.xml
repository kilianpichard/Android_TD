<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
  package="com.example.td1">

  <application
    android:allowBackup="true"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:roundIcon="@mipmap/ic_launcher_round"
    android:supportsRtl="true"
    android:theme="@style/Theme.TD1">
    <activity
      android:name=".AjoutMagasin"
      android:parentActivityName=".ListeMagasins"
      android:theme="@style/Theme.TD1.NoActionBar"
      android:label="@string/title_activity_ajout_magasins"></activity>
    <activity
      android:name=".ListeMagasins"
      android:label="@string/title_activity_liste_magasins"
      android:parentActivityName=".MainActivity"
      android:theme="@style/Theme.TD1.NoActionBar" />
    <activity
      android:name=".ListeProduit"
      android:label="@string/title_activity_liste_produits"
      android:parentActivityName=".MainActivity"
      android:theme="@style/Theme.TD1.NoActionBar" />
    <activity
      android:name=".MainActivity"
      android:label="@string/main_title">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />
        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>
  </application>

</manifest>
